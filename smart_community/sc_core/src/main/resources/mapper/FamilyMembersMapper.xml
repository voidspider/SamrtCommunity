<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sc_core.dao.FamilyMembersMapper">
    <select id="selectFamilyMembersByElderlyId" resultType="FamilyMembers">
        select * from family_members where elderly_id = #{elderlyId};
    </select>
</mapper>